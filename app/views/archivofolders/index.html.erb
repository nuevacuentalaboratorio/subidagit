<p id="notice"><%= notice %></p>

<h1>Listing Archivofolders</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Autor</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @archivofolders.each do |archivofolder| %>
      <tr>
        <td><%= archivofolder.nombre %></td>
        <td><%= archivofolder.autor %></td>
        <td><%= link_to 'Edit', edit_archivofolder_path(archivofolder) %></td>
        <td><%= link_to 'Destroy', archivofolder, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Archivofolder', new_archivofolder_path %>
